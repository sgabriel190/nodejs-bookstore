<% if(utilizator != null) { %>
    <div class="welcome_container">
        <p id="welcome_message">Logat ca
            <%- utilizator %>.
        </p>
    </div>
    <% } %>
        <h1>Raspunsuri chestionar</h1>
        <br />
        <% var i = 0; 
for(var key in raspunsuri) { 
if(raspunsuri[key] == intrebari.listaIntrebari[i].corect) {%>
            <p>Raspunsul la intrebarea
                <%= i+1 %> este corect!</p>
            <% }else{%>
                <p>Raspunsul la intrebarea
                    <%= i+1 %> este gresit!</p>
                <% } %>
                    <p>Raspuns corect:
                        <%- intrebari.listaIntrebari[i].variante[intrebari.listaIntrebari[i].corect]%>
                    </p>
                    <br /><br />
                    <% i++; } %>
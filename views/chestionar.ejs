<% if(utilizator != null) { %>
    <p>Logat ca
        <%- utilizator %>.
    </p>
    <% } %>
        <h1>Chestionar</h1>
        <form action="/rezultat-chestionar" method="post">
            <% for(let i = 0; i<intrebari.listaIntrebari.length; i++) { %>
                <p>
                    <%- intrebari.listaIntrebari[i].intrebare %>
                </p>
                <% for(let j=0;j<Object.keys(intrebari.listaIntrebari[i].variante).length; j++) { %>
                    <input type="radio" name="intrebare_<%- i %>" value="<%- j %>">
                    <label for="<%- intrebari.listaIntrebari[i].variante[j] %>"><%- intrebari.listaIntrebari[i].variante[j] %></label>
                    <br />
                    <% } %>
                        <% } %>
                            <br />
                            <input type="submit" value="Trimite">
        </form>